$("ul.errors").html("");
<% if @structure.errors.any? %>
  <% @structure.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".structures").html("<%= escape_javascript(render 'index') %>");
  $("#structure-modal").modal("hide");
 
<% end %>