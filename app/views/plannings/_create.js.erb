$("ul.errors").html("");
<% if @planning.errors.any? %>
  <% @planning.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".plannings").html("<%= escape_javascript(render 'index') %>");
  $("#planning-modal").modal("hide");
 
<% end %>